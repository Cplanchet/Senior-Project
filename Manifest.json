{
"name": "Link Scanner",
"version": "1.0",
"description": "Scans links for Viruses",
"manifest_version": 2,
"background":{
	"service_worker": "background.js"
},
"browser_action": {
	"default_icon":"icon.png",
	"default_popup":"popup.html"
},
"content_scripts":[
	{
		"matches":[
			"<all_urls>"
		],
		"js":["content.js"]
	}
],
"permissions": [
	"contextMenus",
	"http://127.0.0.1:8080/"
	]
}